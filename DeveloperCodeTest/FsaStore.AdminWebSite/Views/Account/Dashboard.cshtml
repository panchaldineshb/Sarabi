@model FsaStore.Core.Models.DashboardViewModel
@{
    ViewBag.Title = "Dashboard";
}
@section header {
    <hr>
}
@section navbar {
    <div class="masthead">
        <ul class="nav nav-pills pull-right">
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#">About</a></li>
            <li><a href="#">Contact</a></li>
        </ul>
        <h3 class="muted">
            Project name</h3>
    </div>
