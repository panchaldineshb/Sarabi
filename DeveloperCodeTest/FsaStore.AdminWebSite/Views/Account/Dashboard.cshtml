        <p>
            &copy; SarabiStore Development Team @System.DateTime.Now.ToString("yyyy")</p>
    </div>
}
<h2>
    Dashboard</h2>
<p>
    Please enter your username and password to login. @Html.ActionLink("Sign In", "LogOn", "Account")
</p>